{
  "name": "questionsetportal",
  "homepage": "http://localhost:3000",
  "proxy": "http://localhost:3001",
  "version": "0.0.1",
  "environment": "Developer",
  "private": true,
  "port": 3000,
  "scripts": {
    "start": "npm-run-all --parallel open:react open:server",
    "open:server": "npm-run-all --parallel copy-assets run:server",
    "run:server": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./server/server.ts --inspect=12345",
    "open:react": "react-scripts start",
    "build": "npm-run-all build-react compile",
    "build-react": "react-scripts build",
    "copy-assets": "npm-run-all --parallel copy-style-guide copy-bootstrap",
    "copy-style-guide": "npm-run-all --parallel copy-style-guide:fonts copy-style-guide:images copy-style-guide:svgs copy-style-guide:css",
    "copy-style-guide:fonts": "cpx \"node_modules/@avantia/style-guide/fonts/**\" \"./public/assets/fonts\"",
    "copy-style-guide:images": "cpx \"node_modules/@avantia/style-guide/images/**\" \"./public/assets/images\"",
    "copy-style-guide:svgs": "cpx \"node_modules/@avantia/style-guide/svg/**\" \"./public/assets/svg\"",
    "copy-style-guide:css": "cpx \"node_modules/@avantia/style-guide/*.css\" \"./public\"",
    "copy-bootstrap": "cpx \"node_modules/bootstrap/dist/**\" \"./public/bootstrap\"",
    "compile": "tsc",
    "test": "react-scripts test --coverage --testResultsProcessor=jest-teamcity-reporter",
    "server-test": "jest /server/.*\\.test\\.ts",
    "server-test:watch": "jest --watch /server/.*\\.test\\.ts",
    "server-test-compare": "jest /server/nhiIntegration/compareQuotesTool/.*\\.test\\.ts",
    "server-test-compare:watch": "jest --watch /server/nhiIntegration/compareQuotesTool/.*\\.test\\.ts",
    "eject": "react-scripts eject",
    "build-dev-release": "npm-run-all setup:compile setup:prebuild build setup:postbuild setup:copycerts",
    "setup:compile": "echo no compile required",
    "setup:prebuild": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/build-setup.ts \"*\" prebuild",
    "setup:postbuild": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/build-setup.ts \"*\" postbuild",
    "setup:copycerts": "cpx \"./certs/*\" \"./server/certs/\"",
    "test-postbuild": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/build-setup.ts --Developer --postbuild C:\\test-me",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx ./server/**/*.ts ./setup/**/*.ts",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "questionset:upgrade": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/upgrade-question-sets.ts",
    "gen-enum-map": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/generate-qab-enum-map.ts",
    "audit-fix": "npm audit fix --registry=https://registry.npmjs.org/",
    "determine-dependencies": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node ./setup/determine-dependencies.ts"
  },
  "dependencies": {

    "@azure/cosmos": "^3.9.5",
    "@datadog/browser-rum": "^2.5.3",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/react-gtm-module": "^2.0.0",
    "@types/sanitize-html": "^1.27.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.4",
    "dd-trace": "^0.31.0",
    "detect-browser": "^5.2.0",
    "jquery": "^3.5.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.0",
    "react-bootstrap": "^1.5.0",
    "react-datepicker": "^3.5.0",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-hotjar": "^2.2.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-styleguidist": "^10.2.1",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sanitize-html": "^2.3.2",
    "spdy": "^4.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/bootstrap": "^4.3.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cpx": "^1.5.1",
    "@types/eslint": "^6.1.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^24.9.1",
    "@types/jquery": "^3.5.5",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.168",
    "@types/ncp": "^2.0.4",
    "@types/node": "^12.12.5",
    "@types/passport": "^1.0.6",
    "@types/popper.js": "^1.11.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.11",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-datepicker": "^3.1.3",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-immutable-state-invariant": "^2.1.1",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-thunk": "^2.1.0",
    "@types/rimraf": "^3.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/spdy": "^3.4.4",
    "@types/uuid": "^8.3.0",
    "@types/walk": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^9.23.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.20.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest-teamcity-reporter": "^0.9.0",
    "jison": "^0.4.18",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.4.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.5.1",
    "walk": "^2.3.14",
    "webpack-cli": "^3.1.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": "error"
    }
  },
  "browserslist": [
    ">0.1%",
    "not dead",
    "not op_mini all"
  ]
}
