        "validation": [
          {
            "type": "custom",
            "validator": "  var isDayTouched = fields.getData(fieldName + '~Kday')?.isTouched; \n  var isMonthTouched = fields.getData(fieldName + '~Kmonth')?.isTouched; \n  var isYearTouched = fields.getData(fieldName + '~Kyear')?.isTouched; \n  return (isDayTouched && isMonthTouched && isYearTouched) || 'stop-validating'; ",
